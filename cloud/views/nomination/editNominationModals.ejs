                  <button class="btn btn-primary" data-method="clear" type="button" title="Clear">
                    <span class="docs-tooltip" data-toggle="tooltip" title="clear">
                      <span class="icon icon-remove"></span>
                    </span>
                  </button>
                  <button class="btn btn-primary" data-method="reset" type="button" title="Reset">
                    <span class="docs-tooltip" data-toggle="tooltip" title="reset">
                      <span class="icon icon-refresh"></span>
                    </span>
                  </button>
                </div>
              </div>
            </div>
        </div>
         <div class="modal-footer">
           <% if(nominationbackgroundpictureexists) { %>
           <button type="button" id="deleteBackgroundPicture" class="btn btn-danger pull-left">Delete</button>
           <% } %>
           <button id="submitCustomBackgroundPicture" class="btn btn-md btn-primary">Save</button>
           <button type="button" class="btn btn-default pull-right" data-dismiss="modal">Close</button>
         </div>
       </div><!-- /.modal-content -->
     </div><!-- /.modal-dialog -->
   </div><!-- /.modal -->
   <div class="modal fade" id="editDescriptionModal" tabindex="-1" role="dialog" aria-labelledby="editDescriptionModal" aria-hidden="true">
     <div class="modal-dialog modal-lg">
       <div class="modal-content">
         <div class="modal-header">
           <button type="button" class="close" data-dismiss="modal"><span aria-hidden="true">&times;</span><span class="sr-only">Close</span></button>
           <h4 class="modal-title">Edit Description</h4>
         </div>
         <div class="modal-body">
            <form method="POST" action="/sphere/<%= sphereobjectid %>/election/<%= electionobjectid %>/nomination/<%= nominationobjectid %>/editnomination">
            <div class="col-lg-8">
              <fieldset>
                 <div class="form-sphere">
                  <label class="control-label"><%= nominationname %></label>
                  <div class="col-xs-12">
                        <input type="text" id="nominationName" name="nominationName" value="<%= nominationname %>">
                   </div>
                </div>
                <div class="form-sphere">
                  <label class="control-label">Description for <%= nominationname %></label>
                  <div class="col-xs-12">
                        <textarea id="nominationDescriptionEditArea" name="nominationDescription"><%= nominationdescription %></textarea>
                   </div>
                </div>
             </fieldset>
            </div>
            <div class="col-lg-4">
              <div class="well">
                  <h4>Allow Comments on Nomination?</h4>
                  <% if(commentsonnominationallowed) {
                            var commentsonnominationallowedlabel = "Yes";
                        } else {
                            var commentsonnominationallowedlabel = "No";
                        } %>
                  <div class="form-group">
                     <label id="switchcommentsonnominationallowedlabel" class="col-sm-2 control-label"><%= commentsonnominationallowedlabel %></label>
                     <div class="col-sm-10">
                        <label class="switch">
                           <input id="switchcommentsonnominationallowedinput" name="switchcommentsonnominationallowedinput" type="checkbox" <%= switchcommentsonnominationallowedchecked %>>
                           <span></span>
                        </label>
                     </div>
                  </div>
              </div>
            </div>
             <div class="input-sphere">
                <input type="submit" name="submitEditDescription" value="Submit Edit">
             </div>
           </form>
        </div>
         <div class="modal-footer">
            <div class="pull-left">
                <form action="/sphere/<%= sphereobjectid %>/election/<%= electionobjectid %>/nomination/<%= nominationobjectid %>/deletenomination" method="POST">
                    <input type="submit" class="btn btn-sm btn-danger" name="deletenomination" value="Delete">
                </form>
            </div>
            <div class="btn-group pull-right">
               <button type="button" class="btn btn-sm btn-primary" data-toggle="modal" data-target="#addPictureModal" data-dismiss="modal">Add Image</button>
               <button type="button" class="btn btn-sm btn-default" data-dismiss="modal">Close</button>
            </div>
         </div>
       </div><!-- /.modal-content -->
     </div><!-- /.modal-dialog -->
   </div><!-- /.modal -->