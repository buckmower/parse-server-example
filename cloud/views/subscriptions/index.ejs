   <script>
      $(document).ready(function() {
         $("#inviteFriends").click(function() {
            window.top.location.href = 'https://www.facebook.com/dialog/apprequests?app_id=487409348027542&message=Have%20You%20Checked%20Your%20Stats%20Recently%3F&redirect_uri=https://www.represent.xyz/profile';
         });
         $("#userProfile").click(function() {
            window.location.href = '/profile';
         });
         $(".joinedspheredatespan").each(function(){
           var joinedspheredatedata = $(this).data("joinedspheredate");
           var datejoinedlocal = moment(joinedspheredatedata).format('LLL');
           $(this).html("Date Joined: "+datejoinedlocal);
         });
         $(".mostrecentpaymentspan").each(function(){
           var mostrecentpaymentdatedata = $(this).data("mostrecentpaymentdate") || false;
           var joinedspheredatedata = $(this).data("joinedspheredate");
           var unixjoinedspheredate = moment(joinedspheredatedata).unix();
           var unixdatemostrecentpaymentdata = moment(mostrecentpaymentdatedata).unix();
           var nowdatetimeunix = new Date().getTime();
           var eqnumber = $(".mostrecentpaymentspan").index(this);
           if(((nowdatetimeunix - (unixjoinedspheredate * 1000)) < (30 * 24 * 60 * 60 * 1000)) && (mostrecentpaymentdatedata === false)){
            var daysremaining = Math.floor(((1000 * 60 * 60 * 24 * 30) - (nowdatetimeunix - (unixjoinedspheredate * 1000))) / (60 * 60 * 24 * 1000));
            $(this).html("You have "+daysremaining+" days remaining on your trial subscription");
           }
           else if((mostrecentpaymentdatedata !== false) && ((nowdatetimeunix - (unixdatemostrecentpaymentdata * 1000)) < (359 * 24 * 60 * 60 * 1000))){
              var datejoinedlocal = moment(mostrecentpaymentdatedata).format('LLL');
              $(this).html("Paid On: "+datejoinedlocal);
              $(".paysubscriptionform").eq(eqnumber).addClass("hidden");
              $(".subscriptionpaidspan").eq(eqnumber).removeClass("hidden");
            }
            else if((mostrecentpaymentdatedata !== false) && ((nowdatetimeunix - (unixdatemostrecentpaymentdata * 1000)) < (333 * 24 * 60 * 60 * 1000))){
              var daysremaining = Math.floor((nowdatetimeunix - unixjoinedspheredate) / (60 * 60 * 24)) || "Less Than 2";
              $(this).html("Your Subscription Ends In "+daysremaining+" Days!");
            } else if((mostrecentpaymentdatedata !== false) && (nowdatetimeunix - (unixdatemostrecentpaymentdata * 1000) > (365 * 24 * 60 * 60 * 1000))){
              $(this).html("Your Subscription Has Ended.");
            }
         });
      });
   </script>
